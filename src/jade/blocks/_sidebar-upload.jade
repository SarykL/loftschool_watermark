.inputs
  form(id="fileuploads" action="js/libs/upload/server/php/" method="POST" enctype="multipart/form-data")
    .picture__section
      span.picture__section-name
        |Исходное изображение
      span.fake__input
        span.fake__placeholder
          |Загрузите изображение    
      label(for="fileupload", class="").fileupload__label
        input(id="fileupload" type="file" name="files[]" multiple="").fileupload__input-img
      span.error_message
        | Файл не того формата!   
    .watermark__section
      span.watermark__section-name
        |Водяной знак
      span.fake__input
        span.fake__placeholder
          |Загрузите изображение
      label(for="watermark", class="").fileupload__label
        input(type="file", name="name", id="watermark" value="", accept="image/jpeg,image/png,image/gif").fileupload__input-watermark
      span.error_message
        | Файл не того формата!
